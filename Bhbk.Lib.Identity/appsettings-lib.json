{
  "Logging": {
    "IncludeScopes": false,
    "LogLevel": {
      "Default": "Information",
      "System": "Information",
      "Microsoft": "Information"
    }
  },
  "IdentityActivity": {
    "FieldsExcluded": {
      "0": "AccessSuccessCount",
      "1": "AccessFailedCount",
      "2": "ActorId",
      "3": "ConcurrencyStamp",
      "4": "LastUpdated",
      "5": "LastLoginFailure",
      "6": "LastLoginSuccess",
      "7": "SecurityStamp"
    },
    "FieldsSensitive": {
      "0": "ClientKey",
      "1": "ProviderKey",
      "2": "AudienceKey",
      "3": "PasswordHash",
      "4": "ProtectedTicket"
    },
    "TablesExcluded": {
      "0": "AppActivity",
      "1": "AppRoleClaim",
      "2": "AppUserClaim",
      "3": "AppUserLogin",
      "4": "AppUserRefresh",
      "5": "AppUserRole",
      "6": "AppUserToken"
    }
  },
  "IdentityDefaults": {
    "Debug": "false",
    "PasswordLength": "12",
    "AuthorizationCodeLength": "4",
    "AuthorizationCodeLife": "10",
    "AccessTokenLife": "1",
    "FailedAccessAttempts": "10",
    "RefreshTokenLife": "10"
  },
  "IdentityEndpoints": {
    "AdminUrl": "https://localhost:44359",
    "AdminPath": "/api/identity/admin",
    "MeUrl": "https://localhost:44348",
    "MePath": "/api/identity/me",
    "StsUrl": "https://localhost:44375",
    "StsPath": "/api/identity/sts"
  }
